@Article{Fabian2005b,
  title = {Mehr Tempo und Leistung auf der Schiene zwischen Deutschland und Polen},
  author = {Fabian, T.},
  journal = {Eisenbahntechnische Rundschau},
  year = {2005},
  number = {7 \& 8},

  abstract = {Seit mehr als einem Jahr ist die Osterweiterung der Europ{\"a}ischen Union nun Realit{\"a}t. Zeit zur{\"u}ckzublicken? Sicher nicht! H{\"o}chste Zeit aber, um nach der Realit{\"a}t der Erweiterung gerade im Hinblick auf die Verkehrsverbindungen zu fragen und den Stand der wichtigsten Ost- West-Relationen zwischen Deutschland und Polen zu beleuchten. Im nachfolgenden Beitrag soll daher der Frage nachgegangen werden, wo der Schienenverkehr zwischen Deutschland und Polen im Jahr "2" der EU-Erweiterung steht, wo Defizite liegen, wieweit Planungen vorangeschritten sind und welcher Handlungsbedarf nach wie vor besteht. Die Schienenverbindungen zwischen Deutschland und Polen sind in den Jahrzehnten vor der politischen Wende in Europa erheblich vernachl{\"a}ssigt worden und waren zu Beginn der 1990er Jahre gr{\"o}{\"s}tenteils in einem beklagenswerten Zustand. Bei der Modernisierung dieser Verbindungen sind in den vergangenen Jahren, insbesondere in Polen, erhebliche Fortschritte gemacht worden. Doch der fehlende oder nur schleppende Aus- bzw. Neubau zentraler grenz{\"u}berschreitender Infrastrukturen steht attraktiven Reisezeiten noch auf absehbare Zeit im Wege. Daneben bestehen auch heute noch eine Reihe von rechtlichen, administrativen und organisatorischen Hemmnissen, die die ohnehin langwierigen Planungen nachhaltig zu verz{\"o}gern drohen.},
  file = {Fabian2005Polen-short:2005\\Fabian2005Polen-short.pdf:PDF}
}

@InBook{Fabian2006,
  title = {Grenz{\"u}berschreitende Planungsverfahren zwischen Deutschland und Polen},
  author = {Fabian, T.},
  editor = {Born, K. M.; Fichtner, T.; Kr{\"a}tke, S.;},
  publisher = {Akademie f{\"u}r Raumforschung und Landesplanung},
  year = {2006},

  abstract = {Die Planungsverfahren f{\"u}r Verkehrswege sind in den verschiedenen Staaten Europas von erheblichen Unterschieden gekennzeichnet. Bereits bei rein nationalen Infrastrukturprojekten erfordern die Planungen ein hohes Ma{\"s} an Koordination zwischen einer Vielzahl von Institutionen und Akteuren. In Grenzregionen und {\"u}ber nationale Grenzen hinweg stellt sich die Planung derartiger Verkehrsprojekte als eine ungleich gr{\"o}{\"s}ere Herausforderung dar. Die beteiligten Institutionen und Akteure beiderseits der Grenze stehen hier trotz h{\"a}ufig {\"a}hnlicher oder gemeinsamer Interessen vor dem Problem gemeinsame Entscheidungen herbeif{\"u}hren zu m{\"u}ssen, ohne dabei auf gemeinsame Strukturen und Verfahren zur{\"u}ckgreifen zu k{\"o}nnen.},

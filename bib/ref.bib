@InProceedings{Figueroa1998SantaCruz,
  title = {Santa Cruz de la Sierra Urban Public Transport System, Urban Structure and Network Restructuring},
  author = {Figueroa, O. and Pizarro, A.},
  booktitle = {Urban Transport Policy: A Sustainable Development Tool; proceedings of the International Conference CODATU VIII, Cape Town, South Africa, 21-25 September 1998},
  year = {1998},

  address = {Rotterdam},
  editor = {Freeman, P. and Jamet, C. and Freenab, P.},
  pages = {347--352},
  publisher = {Balkema},

  abstract = {Santa Cruz de la Sierra in Bolivia has experienced rapid demographic growth in the past 20 years, becoming the second largest city in the country. This growth has produced a significant extension of the city and an increase in transportation demand. This paper presents an institutional, operational, and functional analysis of public transit in Santa Cruz, as well as proposals put forward to improve the situation. The proposal and recommendations include the creation of a municipal authority that concentrates all functions relative to public transit. This authority should have the resources and capabilities to develop a more effective regulation of the sector, through the concession of routes to operators. For the operators, it is considered essential that the organizational structure be modified to generate a greater level of professionalism. It was recommended that the existing professional associations be transformed into real companies, keeping the positive aspects of the existing set up. Finally, a network restructuring proposal is made that takes into consideration public transit demand and the urban structure of the city.},
  comment = {Urban Transport Policy: A Sustainable Development ToolCODATU VIII International Scientific CommitteeCape Town, South Africa StartDate:19980921 EndDate:19980925 Sponsors:CODATU VIII International Scientific Committee},
  file = {Figueroa1998SantaCruz.pdf:1998/Figueroa1998SantaCruz.pdf:PDF},

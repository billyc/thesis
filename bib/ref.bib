@Article{ZhangJin2002KinematicWaveModelMixedTraffic,
  title = {Kinematic wave traffic flow model for mixed traffic},
  author = {Zhang, H. M. and Jin, W. L.},
  journal = {Transportation Research Record: Journal of the Transportation Research Board},

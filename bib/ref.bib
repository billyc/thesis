@MastersThesis{Titze2007EntwicklungeinesOeV-Routingmoduls,
  title = {{Entwicklung eines {\"O}V-Routingmoduls f{\"u}r Multiagentensimulationen}},
  author = {Titze, Torben},
  school = {TU Berlin},
  year = {2007},
  type = {Term Project Report},

  abstract = {Die Simulation von Verkehrsnetzen mit Hilfe von agentenbasierten Modellen hat unter anderem zum Ziel, die Verkehrsmittelwahl in die Simulation mit einzubeziehen. Soll diese Wahl nicht nur aufgrund eines vorgegeben Modal Splits getroffen werden, ist es n{\"o}tig ein Modell des {\"o}ffentlichen Verkehrs in die Simulation zu integrieren. Diese Studienarbeit hat die Zielsetzung, aus gegebenen Fahrplandaten f{\"u}r den Raum Berlin ein Netzmodell zu erarbeiten und f{\"u}r dieses Netz einen fahrplanfeinen Router auf Basis des Dijkstra-Algorithmus zu entwickeln. Dieser Router soll in das {MATSim}-Programmpaket integriert werden.},

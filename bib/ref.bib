@TechReport{Ben-AkivaEtc2004MITSIMCalibFramework,
  title = {Calibration of Microscopic Traffic Simulation Models with Aggregate Data},
  author = {Ben-Akiva, M. and Darda, D. and Jha, M. and Koutsopoulos, H.N. and Toledo, T.},
  year = {2004},

  abstract = {This paper presents a framework for the calibration of microscopic traffic simulation models usingaggregate data. The framework takes into account the interactions between the various model parameters and the OD flows by estimating OD flows jointly with the model parameters. An optimization-based approach has been used for the joint calibration. A systematic search approach based on the Box algorithmis adopted for the solution of the resulting minimization problem. OD estimation is based on the generalized least squares (GLS) estimator. Since the calibration of the parameters depends on the estimatedOD flows and vice versa, the proposed framework is iterative. The applicability of the approach is demonstrated through its application to case studies using MITSIMLab, a microscopic traffic simulation model.},

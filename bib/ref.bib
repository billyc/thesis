@Article{Chriqui1975CommonLinesProblem,
  title = {Common Bus Lines},
  author = {Chriqui, C. and Robillard, P.},
  journal = {Transportation Science},
  year = {1975},
  number = {2},
  pages = {115--121},
  volume = {9},

  abstract = {The article claims that a passenger in A will not use just any of a number of available routes. Because some routes might have a very long travel time to reach B, he may disregard them. In that way, the passenger operates a selection: among the set of all routes passing through A and B, he will select a subset so as to minimize his total expected travel time, which is made up of his expected waiting time plus his expected travel time. Once this subset is selected, the passenger will take the first vehicle serving one of the routes in this subset to arrive at A. The purpose of this paper is to present a probabilistic framework for the study of this problem, which takes into account the frequencies and the speeds of each competing A. In this paper the problem of the determination of the expected travel time is regarded as an optimization problem. The approach used allows the consideration of any distribution function for first arrival time of a vehicle on a route. In practice, the cases},
  doi = {10.1287/trsc.9.2.115},
  file = {Chriqui1975CommonLinesProblem.pdf:1975\\Chriqui1975CommonLinesProblem.pdf:PDF},
  issn = {00411655},
  keywords = {BUS lines, COMMUTING, TRANSPORTATION, COMMUNICATIONS industries, PUBLIC transit, URBAN transportation, RIDESHARING, AUTOMOTIVE transportation}
}

